<h1>Welcome to BlogSpot</h1>
<a href="/blogs/new">Add new blog</a>
<% blogs.forEach(blog=>{ %>
    <li><div>
        <h4><%= blog.title %></h4>
        <img src="<%= blog.image %>">
        <p>Created: <%= blog.created %></p>
        <p><%= blog.body %></p>
        <a href="/blogs/<%=blog._id %>">OPEN!</a>
    </div></li>
<% }); %> 