<h1>Edit <%= blog.title %></h1>
<form action="/blogs/<%= blog._id %>?_method=PUT" method="POST">
    <input name="blog[title]" value="<%= blog.title %>" type="text">
    <input name="blog[image]" value="<%= blog.image %>" type="text">
    <input name="blog[body]" value="<%= blog.body %>" type="text">
    <button>Submit</button>
</form>